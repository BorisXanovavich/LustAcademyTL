# game/logic/locations/restroom.rpy:2
translate deutsch restroom_stump_label_6bcbc6d0:

    # ch_Name "{i}(Am I sure there's nothing I want to do before going downstairs?!){/i}"
    ch_Name "{i}(Bin ich mir sicher, dass ich nichts mehr tun will, bevor ich nach unten gehe?!){/i}"

# game/logic/locations/restroom.rpy:13
translate deutsch restroom_label_2b70a15b:

    # ch_Name "Don, hi!"
    ch_Name ""

# game/logic/locations/restroom.rpy:15
translate deutsch restroom_label_ecefc0d1:

    # ch_Name "How's it going, how's work?"
    ch_Name "Wie geht's, wie steht's mit der Arbeit?"

# game/logic/locations/restroom.rpy:17
translate deutsch restroom_label_5a5f8bdc:

    # Don "Get to the point, [Name]."
    Don "Komm zum Punkt, [Name]."

# game/logic/locations/restroom.rpy:19
translate deutsch restroom_label_3b0c1b5a:

    # Don "Don't waste my time."
    Don "Verschwende nicht meine Zeit."

# game/logic/locations/restroom.rpy:21
translate deutsch restroom_label_d4167a3d:

    # Don "Baseball's about to start, so hurry up and talk..."
    Don "Das Baseballspiel fängt gleich an, also beeil dich und rede..."

# game/logic/locations/restroom.rpy:23
translate deutsch restroom_label_3eb81665:

    # Don "What do you want?"
    Don "Was willst du?"

# game/logic/locations/restroom.rpy:25
translate deutsch restroom_label_0c152f26:

    # ch_Name "The thing is, Olivia came to see me..."
    ch_Name "Die Sache ist die, dass Olivia zu mir gekommen ist..."

# game/logic/locations/restroom.rpy:27
translate deutsch restroom_label_ccfe8b04:

    # Don "And?"
    Don "Und?"

# game/logic/locations/restroom.rpy:29
translate deutsch restroom_label_58012455:

    # ch_Name "And? I'd like to discuss your rental idea..."
    ch_Name "Und? Ich würde gerne über deine Mietidee sprechen..."

# game/logic/locations/restroom.rpy:31
translate deutsch restroom_label_81537022:

    # Don "It's not an idea."
    Don "Es ist keine Idee."

# game/logic/locations/restroom.rpy:33
translate deutsch restroom_label_a410d3a2:

    # Don "It's a fact you've been informed about."
    Don "Das ist eine Tatsache, über die du informiert worden bist."

# game/logic/locations/restroom.rpy:35
translate deutsch restroom_label_0aa19b5e:

    # Don "If you don't want to study, then work."
    Don "Wenn du nicht studieren willst, dann arbeite."

# game/logic/locations/restroom.rpy:37
translate deutsch restroom_label_f5f74f6c:

    # Don "And since you're 18 and working..."
    Don "Und da du 18 bist und arbeitest..."

# game/logic/locations/restroom.rpy:39
translate deutsch restroom_label_e0555c31:

    # Don "Why should I sweat for another grown man?"
    Don "Warum sollte ich für einen anderen erwachsenen Mann schuften?"

# game/logic/locations/restroom.rpy:41
translate deutsch restroom_label_14fd1ab9:

    # Don "So pay up or get out."
    Don "Also zahle oder verschwinde."

# game/logic/locations/restroom.rpy:43
translate deutsch restroom_label_626491b9:

    # Don "Anything else?"
    Don "Noch irgendwas?"

# game/logic/locations/restroom.rpy:45
translate deutsch restroom_label_1fb30e0f:

    # ch_Name "But hang on, Don- I still want to study!"
    ch_Name "Aber warte mal, Don- Ich will doch studieren!"

# game/logic/locations/restroom.rpy:47
translate deutsch restroom_label_fd5a0c7e:

    # Don "I don't see you packing a suitcase full of books."
    Don "Ich sehe aber nicht, dass du einen Koffer mit Büchern packst."

# game/logic/locations/restroom.rpy:49
translate deutsch restroom_label_6f619831:

    # ch_Name "You know I had a sports scholarship..."
    ch_Name "Du weißt, dass ich ein Sportstipendium hatte..."

# game/logic/locations/restroom.rpy:51
translate deutsch restroom_label_dce7c30c:

    # Don "So?"
    Don "Und?"

# game/logic/locations/restroom.rpy:53
translate deutsch restroom_label_9c8b6339:

    # ch_Name "If it wasn't for the injury, then..."
    ch_Name "Wenn die Verletzung nicht gewesen wäre, dann..."

# game/logic/locations/restroom.rpy:55
translate deutsch restroom_label_e3052bd7:

    # Don "There are no \"If’s\" in history, boy."
    Don "Es gibt keine \"Wenns\" in der Geschichte, Junge."

# game/logic/locations/restroom.rpy:57
translate deutsch restroom_label_5e4ace9f:

    # Don "You're 18. You don't study and you're sitting on my neck."
    Don "Du bist 18. Du studierst nicht und sitzt mir im Nacken."

# game/logic/locations/restroom.rpy:59
translate deutsch restroom_label_2572973c:

    # Don "I'm tired of it. I won't allow it anymore."
    Don "Ich habe es satt. Ich werde es nicht mehr zulassen."

# game/logic/locations/restroom.rpy:61
translate deutsch restroom_label_cf56f434:

    # Don "These are the facts we have."
    Don "Dies sind die Fakten, die uns vorliegen..."

# game/logic/locations/restroom.rpy:69
translate deutsch restroom_label_682b054f:

    # ch_Name "Let me try to change your mind!"
    ch_Name "Lass mich versuchen, deine Meinung zu ändern!"

# game/logic/locations/restroom.rpy:71
translate deutsch restroom_label_fcb78012:

    # ch_Name "I'm not sitting idle at all!"
    ch_Name "Ich bin keineswegs untätig!"

# game/logic/locations/restroom.rpy:73
translate deutsch restroom_label_abf076ed:

    # ch_Name "And I've been working all summer just to get an education."
    ch_Name "Und ich habe den ganzen Sommer gearbeitet, nur um eine Weiterbildung zu bekommen."

# game/logic/locations/restroom.rpy:75
translate deutsch restroom_label_f6015940:

    # ch_Name "To pay for college. You know what I mean?"
    ch_Name "Um das College zu bezahlen. Weißt du, was ich meine?"

# game/logic/locations/restroom.rpy:77
translate deutsch restroom_label_5e01fdd4:

    # Don "Okay. And?"
    Don "Okay. Und?"

# game/logic/locations/restroom.rpy:79
translate deutsch restroom_label_0a168ce2:

    # ch_Name "I was just working to pay for college."
    ch_Name "Ich habe nur gearbeitet, um das College zu bezahlen."

# game/logic/locations/restroom.rpy:81
translate deutsch restroom_label_b5777121:

    # ch_Name "And you want me to either study or pay you, right?"
    ch_Name "Und du willst, dass ich entweder studiere oder dich bezahle, richtig?"

# game/logic/locations/restroom.rpy:83
translate deutsch restroom_label_820cafde:

    # Don "You get the point."
    Don "Du verstehst, worum es geht."

# game/logic/locations/restroom.rpy:85
translate deutsch restroom_label_08b46c47:

    # ch_Name "But!"
    ch_Name "Aber!"

# game/logic/locations/restroom.rpy:87
translate deutsch restroom_label_53b0ab25:

    # ch_Name "If I have to pay your rent."
    ch_Name "Wenn ich deine Miete bezahlen muss, dann..."

# game/logic/locations/restroom.rpy:89
translate deutsch restroom_label_16f04a0d:

    # ch_Name "I'll never save up to go to collegel!"
    ch_Name "werde ich nie genügend Geld haben, um auf ein College zu gehen!"

# game/logic/locations/restroom.rpy:91
translate deutsch restroom_label_8d9ed8c8:

    # ch_Name "And I'll have to keep paying you."
    ch_Name "Und ich muss dich weiter bezahlen."

# game/logic/locations/restroom.rpy:93
translate deutsch restroom_label_3bb46233:

    # ch_Name "It's a vicious circle. You can't be serious... I mean..."
    ch_Name "Es ist ein Teufelskreis. Das kann doch nicht dein Ernst sein... Ich meine..."

# game/logic/locations/restroom.rpy:95
translate deutsch restroom_label_57e413fb:

    # ch_Name "What do you think? I'm sure you can understand my situation..."
    ch_Name "Was meinst du dazu? Ich bin sicher, du könntest meine Situation verstehen..."

# game/logic/locations/restroom.rpy:97
translate deutsch restroom_label_18b25c53:

    # Don "Of course I can. But I don't care."
    Don "Natürlich kann ich das. Aber es ist mir egal."

# game/logic/locations/restroom.rpy:99
translate deutsch restroom_label_8203ea8f:

    # Don "It's time you learned how to be independent, kid."
    Don "Es wird Zeit, dass du lernst, unabhängig zu sein, Junge."

# game/logic/locations/restroom.rpy:101
translate deutsch restroom_label_e04485a7:

    # Don "Lesson one: you're 18 and your problems are your problems."
    Don "Lektion eins: Du bist 18 und deine Probleme sind deine Probleme."

# game/logic/locations/restroom.rpy:103
translate deutsch restroom_label_d1e1ef88:

    # Don "The world doesn't revolve around you."
    Don "Die Welt dreht sich nicht nur um dich."

# game/logic/locations/restroom.rpy:105
translate deutsch restroom_label_89df8599:

    # Don "Don't put your issues on me."
    Don "Schiebe deine Probleme nicht auf mich."

# game/logic/locations/restroom.rpy:107
translate deutsch restroom_label_a99eee17:

    # Don "Pay your rent or find yourself a new place to live. What's it gonna be?"
    Don "Zahle deine Miete oder such dir eine neue Bleibe. Wie wirst du dich jetzt entscheiden?"

# game/logic/locations/restroom.rpy:112
translate deutsch restroom_menu_2_label_527c060b:

    # ch_Name "Are you sure you got all the facts straight, Don?!"
    ch_Name "Bist du dir sicher, dass du alle Fakten richtig verstanden hast, Don?!"

# game/logic/locations/restroom.rpy:114
translate deutsch restroom_menu_2_label_f87461fa:

    # ch_Name "Maybe I should remind you that I could have been at university now."
    ch_Name "Vielleicht sollte ich dich daran erinnern, dass ich jetzt an der Universtität hätte sein können."

# game/logic/locations/restroom.rpy:116
translate deutsch restroom_menu_2_label_0843679a:

    # ch_Name "If someone hadn't saved money on my rehabilitation."
    ch_Name "Wenn nicht jemand bei meiner Genesung gespart hätte, wäre ich jetzt nicht hier."

# game/logic/locations/restroom.rpy:118
translate deutsch restroom_menu_2_label_0252acee:

    # ch_Name "I'm not saying you had to..."
    ch_Name "Ich sage nicht, dass du musstest..."

# game/logic/locations/restroom.rpy:120
translate deutsch restroom_menu_2_label_8d965803:

    # Don "That's exactly what you're saying right now!"
    Don "Das ist genau das, was du gerade sagst!"

# game/logic/locations/restroom.rpy:122
translate deutsch restroom_menu_2_label_01f136c0:

    # Don "And I suggest you shut up and think about your tone."
    Don "Und ich schlage vor, dass du dein Mund hälst und über dein Ton nachdenkst."

# game/logic/locations/restroom.rpy:124
translate deutsch restroom_menu_2_label_d80e228a:

    # Don "Where would you be if it wasn't for me?"
    Don "Wo wärst du, wenn es mich nicht gäbe?"

# game/logic/locations/restroom.rpy:126
translate deutsch restroom_menu_2_label_363df24c:

    # Don "Haven't I worked hard enough to give you food, clothes and an education?"
    Don "Habe ich nicht schon hart genug gearbeitet, um dir Essen, Kleidung und eine Schulbildung zu ermöglichen?"

# game/logic/locations/restroom.rpy:128
translate deutsch restroom_menu_2_label_32fefd64:

    # Don "Do you have any idea how much your rehabilitation would have cost?"
    Don "Hast du eine Ahnung, wie viel deine Rehabilitation gekostet hätte?"

# game/logic/locations/restroom.rpy:130
translate deutsch restroom_menu_2_label_c152ecb2:

    # ch_Name "No."
    ch_Name "Nein."

# game/logic/locations/restroom.rpy:132
translate deutsch restroom_menu_2_label_bbb98143:

    # ch_Name "All I’m saying is that I didn’t \"choose\" not to go to university."
    ch_Name "Ich will damit nur sagen, dass ich es nicht \"entschieden\" habe, nicht zu studieren."

# game/logic/locations/restroom.rpy:134
translate deutsch restroom_menu_2_label_3fe05916:

    # ch_Name "It's hard to keep your emotions in check when you're being slung in the face."
    ch_Name "Es ist schwer, seine Emotionen im Zaum zu halten, wenn einem ins Gesicht gespuckt wird."

# game/logic/locations/restroom.rpy:136
translate deutsch restroom_menu_2_label_2d95ecb9:

    # Don "Oh, you poor thing!"
    Don "Oh, du armes Ding!"

# game/logic/locations/restroom.rpy:138
translate deutsch restroom_menu_2_label_6c4d7e3c:

    # Don "Is that what you were expecting to hear?"
    Don "Ist es das, was du erwartet hast zu hören?"

# game/logic/locations/restroom.rpy:140
translate deutsch restroom_menu_2_label_34919b9e:

    # ch_Name "No, I..."
    ch_Name "Nein, Ich..."

# game/logic/locations/restroom.rpy:142
translate deutsch restroom_menu_2_label_0d4f4111:

    # Don "You're 18! Surprise! Nobody owes you anything."
    Don "Du bist 18! Überraschung! Niemand schuldet dir irgendwas."

# game/logic/locations/restroom.rpy:144
translate deutsch restroom_menu_2_label_fbfeb7e0:

    # Don "Certainly not me."
    Don "Ich jedenfalls nicht."

# game/logic/locations/restroom.rpy:146
translate deutsch restroom_menu_2_label_f6e4a5c9:

    # Don "So stop whining and learn to be helpful."
    Don "Also hör auf zu weinen und lern, hilfreich zu sein."

# game/logic/locations/restroom.rpy:148
translate deutsch restroom_menu_2_label_d39ba59a:

    # ch_Name "I hear you."
    ch_Name "Verstehe."

# game/logic/locations/restroom.rpy:150
translate deutsch restroom_menu_2_label_02514dd4:

    # Don "So it's a deal, then?"
    Don "Also ist es abgemacht?"

# game/logic/locations/restroom.rpy:156
translate deutsch restroom_menu_3_label_1b0b05ae:

    # ch_Name "I... I need to think."
    ch_Name "Ich... Ich muss nachdenken."

# game/logic/locations/restroom.rpy:158
translate deutsch restroom_menu_3_label_4af10fbd:

    # ch_Name "I'm sorry, I'm gonna..."
    ch_Name "Tut mir leid. Ich werde..."

# game/logic/locations/restroom.rpy:160
translate deutsch restroom_menu_3_label_c5b7df6d:

    # Don "Go on, get out of here."
    Don "Los, raus mit dir."

# game/logic/locations/restroom.rpy:162
translate deutsch restroom_menu_3_label_1f8ee931:

    # Don "And welcome to adulthood, boy!"
    Don "Und willkommen im Erwachsensein, Junge!"

# game/logic/locations/restroom.rpy:164
translate deutsch restroom_menu_3_label_f708fe02:

    # ch_Name "{i}(Shove your \"welcome to adulthood\" up your ass.){/i}"
    ch_Name "{i}(Schieb dir dein \"Willkommen im Erwachsensein\" in den Arsch.){/i}"

# game/logic/locations/restroom.rpy:166
translate deutsch restroom_menu_3_label_41ea1485:

    # ch_Name "{i}(Fucking hypocrite.){/i}"
    ch_Name "{i}(Scheiß Heuchler.){/i}"

# game/logic/locations/restroom.rpy:168
translate deutsch restroom_menu_3_label_24687b0f:

    # ch_Name "{i}(I wonder how you'd act if it were Ashley who got hurt instead?){/i}"
    ch_Name "{i}(Ich frage mich, wie du dich verhalten würdest, wenn Ashley stattdessen verletzt worden wäre?){/i}"

