# game/logic/locations/restroom.rpy:2
translate philippine restroom_stump_label_6bcbc6d0:

    # ch_Name "{i}(Am I sure there's nothing I want to do before going downstairs?!){/i}"
    ch_Name "{i}(Sigurado ba akong wala akong gustong gawin bago ako bumaba?!){/i}"

# game/logic/locations/restroom.rpy:13
translate philippine restroom_label_2b70a15b:

    # ch_Name "Don, hi!"
    ch_Name ""

# game/logic/locations/restroom.rpy:15
translate philippine restroom_label_ecefc0d1:

    # ch_Name "How's it going, how's work?"
    ch_Name "Kamusta ka na, ang trabaho?"

# game/logic/locations/restroom.rpy:17
translate philippine restroom_label_5a5f8bdc:

    # Don "Get to the point, [Name]."
    Don "Sabihin mo na, [Name]."

# game/logic/locations/restroom.rpy:19
translate philippine restroom_label_3b0c1b5a:

    # Don "Don't waste my time."
    Don "Huwag mong sayangin ang oras ko."

# game/logic/locations/restroom.rpy:21
translate philippine restroom_label_d4167a3d:

    # Don "Baseball's about to start, so hurry up and talk..."
    Don "Magsisimula na ang baseball, kaya bilisan mo..."

# game/logic/locations/restroom.rpy:23
translate philippine restroom_label_3eb81665:

    # Don "What do you want?"
    Don "Ano ang gusto mo?"

# game/logic/locations/restroom.rpy:25
translate philippine restroom_label_0c152f26:

    # ch_Name "The thing is, Olivia came to see me..."
    ch_Name "Pumunta sa akin si Olivia..."

# game/logic/locations/restroom.rpy:27
translate philippine restroom_label_ccfe8b04:

    # Don "And?"
    Don "At?"

# game/logic/locations/restroom.rpy:29
translate philippine restroom_label_58012455:

    # ch_Name "And? I'd like to discuss your rental idea..."
    ch_Name "At? Gusto kong pag-usapan ang iyong ideya sa pag-upa..."

# game/logic/locations/restroom.rpy:31
translate philippine restroom_label_81537022:

    # Don "It's not an idea."
    Don "Hindi ideya 'yun."

# game/logic/locations/restroom.rpy:33
translate philippine restroom_label_a410d3a2:

    # Don "It's a fact you've been informed about."
    Don "Iyon ay katotohanan na ipinaalam sa iyo."

# game/logic/locations/restroom.rpy:35
translate philippine restroom_label_0aa19b5e:

    # Don "If you don't want to study, then work."
    Don "Kung ayaw mong mag-aral, magtrabaho ka."

# game/logic/locations/restroom.rpy:37
translate philippine restroom_label_f5f74f6c:

    # Don "And since you're 18 and working..."
    Don "At dahil 18 ka na at nagtatrabaho..."

# game/logic/locations/restroom.rpy:39
translate philippine restroom_label_e0555c31:

    # Don "Why should I sweat for another grown man?"
    Don "Bakit ako magpapawis para sa ibang lalaki?"

# game/logic/locations/restroom.rpy:41
translate philippine restroom_label_14fd1ab9:

    # Don "So pay up or get out."
    Don "Kaya magbayad ka o lumabas ka."

# game/logic/locations/restroom.rpy:43
translate philippine restroom_label_626491b9:

    # Don "Anything else?"
    Don "May iba pa?"

# game/logic/locations/restroom.rpy:45
translate philippine restroom_label_1fb30e0f:

    # ch_Name "But hang on, Don- I still want to study!"
    ch_Name "Teka, Don- gusto ko pang mag-aral!"

# game/logic/locations/restroom.rpy:47
translate philippine restroom_label_fd5a0c7e:

    # Don "I don't see you packing a suitcase full of books."
    Don "Hindi kita nakikitang nag-iimpake ng maleta na puno ng mga libro."

# game/logic/locations/restroom.rpy:49
translate philippine restroom_label_6f619831:

    # ch_Name "You know I had a sports scholarship..."
    ch_Name "Alam mo may sports scholarship ako..."

# game/logic/locations/restroom.rpy:51
translate philippine restroom_label_dce7c30c:

    # Don "So?"
    Don "At?"

# game/logic/locations/restroom.rpy:53
translate philippine restroom_label_9c8b6339:

    # ch_Name "If it wasn't for the injury, then..."
    ch_Name "Kung hindi dahil sa pinsala..."

# game/logic/locations/restroom.rpy:55
translate philippine restroom_label_e3052bd7:

    # Don "There are no \"If’s\" in history, boy."
    Don "Walang \"Kung\" sa kasaysayan."

# game/logic/locations/restroom.rpy:57
translate philippine restroom_label_5e4ace9f:

    # Don "You're 18. You don't study and you're sitting on my neck."
    Don "18 ka na. Hindi ka nag-aaral at nakaupo ka sa leeg ko."

# game/logic/locations/restroom.rpy:59
translate philippine restroom_label_2572973c:

    # Don "I'm tired of it. I won't allow it anymore."
    Don "Pagod na ako. Hindi na ako papayag."

# game/logic/locations/restroom.rpy:61
translate philippine restroom_label_cf56f434:

    # Don "These are the facts we have."
    Don "Ito ang mga katotohanan dito."

# game/logic/locations/restroom.rpy:69
translate philippine restroom_label_682b054f:

    # ch_Name "Let me try to change your mind!"
    ch_Name "Payagan mong subukan kong baguhin ang isip mo!"

# game/logic/locations/restroom.rpy:71
translate philippine restroom_label_fcb78012:

    # ch_Name "I'm not sitting idle at all!"
    ch_Name "Hindi ako nakaupong walang ginagawa!"

# game/logic/locations/restroom.rpy:73
translate philippine restroom_label_abf076ed:

    # ch_Name "And I've been working all summer just to get an education."
    ch_Name "At buong tag-init akong nagtatrabaho para lang makapag-aral."

# game/logic/locations/restroom.rpy:75
translate philippine restroom_label_f6015940:

    # ch_Name "To pay for college. You know what I mean?"
    ch_Name "Para magbayad sa kolehiyo. You know what I mean?"

# game/logic/locations/restroom.rpy:77
translate philippine restroom_label_5e01fdd4:

    # Don "Okay. And?"
    Don "Sige. At?"

# game/logic/locations/restroom.rpy:79
translate philippine restroom_label_0a168ce2:

    # ch_Name "I was just working to pay for college."
    ch_Name "Nagtatrabaho ako para may pambayad sa kolehiyo."

# game/logic/locations/restroom.rpy:81
translate philippine restroom_label_b5777121:

    # ch_Name "And you want me to either study or pay you, right?"
    ch_Name "At gusto mo ako ang mag-aral o magbayad sa iyo, diba?"

# game/logic/locations/restroom.rpy:83
translate philippine restroom_label_820cafde:

    # Don "You get the point."
    Don "Bilisan mo."

# game/logic/locations/restroom.rpy:85
translate philippine restroom_label_08b46c47:

    # ch_Name "But!"
    ch_Name "Pero!"

# game/logic/locations/restroom.rpy:87
translate philippine restroom_label_53b0ab25:

    # ch_Name "If I have to pay your rent."
    ch_Name "Kung kailangan kong magbayad ng upa mo."

# game/logic/locations/restroom.rpy:89
translate philippine restroom_label_16f04a0d:

    # ch_Name "I'll never save up to go to collegel!"
    ch_Name "Hinding-hindi ako mag-iipon para makapag-aral sa kolehiyo!"

# game/logic/locations/restroom.rpy:91
translate philippine restroom_label_8d9ed8c8:

    # ch_Name "And I'll have to keep paying you."
    ch_Name "At kailangan kong patuloy na magbayad sa iyo."

# game/logic/locations/restroom.rpy:93
translate philippine restroom_label_3bb46233:

    # ch_Name "It's a vicious circle. You can't be serious... I mean..."
    ch_Name "Mag-iikot-ikot lang ako. Seryoso ka ba... Ibig kong sabihin..."

# game/logic/locations/restroom.rpy:95
translate philippine restroom_label_57e413fb:

    # ch_Name "What do you think? I'm sure you can understand my situation..."
    ch_Name "Ano sa tingin mo? Sigurado akong maiintindihan mo ang sitwasyon ko..."

# game/logic/locations/restroom.rpy:97
translate philippine restroom_label_18b25c53:

    # Don "Of course I can. But I don't care."
    Don "Siyempre. Pero wala akong pakialam."

# game/logic/locations/restroom.rpy:99
translate philippine restroom_label_8203ea8f:

    # Don "It's time you learned how to be independent, kid."
    Don "Panahon na para matuto ka na nagsasarili ka."

# game/logic/locations/restroom.rpy:101
translate philippine restroom_label_e04485a7:

    # Don "Lesson one: you're 18 and your problems are your problems."
    Don "Unang aralin: 18 ka na at ang iyong mga problema ay ang iyong mga problema."

# game/logic/locations/restroom.rpy:103
translate philippine restroom_label_d1e1ef88:

    # Don "The world doesn't revolve around you."
    Don "Hindi sa iyo umiikot ang mundo."

# game/logic/locations/restroom.rpy:105
translate philippine restroom_label_89df8599:

    # Don "Don't put your issues on me."
    Don "Huwag mong idamay sa akin ang mga isyu mo."

# game/logic/locations/restroom.rpy:107
translate philippine restroom_label_a99eee17:

    # Don "Pay your rent or find yourself a new place to live. What's it gonna be?"
    Don "Magbayad ka ng renta mo o humanap ka ng bagong tirahan. Ano kaya?"

# game/logic/locations/restroom.rpy:112
translate philippine restroom_menu_2_label_527c060b:

    # ch_Name "Are you sure you got all the facts straight, Don?!"
    ch_Name "Sigurado ka bang tama ka sa lahat, Don?!"

# game/logic/locations/restroom.rpy:114
translate philippine restroom_menu_2_label_f87461fa:

    # ch_Name "Maybe I should remind you that I could have been at university now."
    ch_Name "Siguro dapat kong ipaalala sa iyo na sana nasa unibersidad na ako ngayon."

# game/logic/locations/restroom.rpy:116
translate philippine restroom_menu_2_label_0843679a:

    # ch_Name "If someone hadn't saved money on my rehabilitation."
    ch_Name "Kung may taong hindi nag-ipon ng pera sa aking rehabilitasyon."

# game/logic/locations/restroom.rpy:118
translate philippine restroom_menu_2_label_0252acee:

    # ch_Name "I'm not saying you had to..."
    ch_Name "Hindi ko sinasabing kailangan mong..."

# game/logic/locations/restroom.rpy:120
translate philippine restroom_menu_2_label_8d965803:

    # Don "That's exactly what you're saying right now!"
    Don "'Yan talaga ang sinasabi mo ngayon!"

# game/logic/locations/restroom.rpy:122
translate philippine restroom_menu_2_label_01f136c0:

    # Don "And I suggest you shut up and think about your tone."
    Don "At siguro tumahimik ka at pag-isipan ang iyong tono."

# game/logic/locations/restroom.rpy:124
translate philippine restroom_menu_2_label_d80e228a:

    # Don "Where would you be if it wasn't for me?"
    Don "Nasaan ka ba kung hindi dahil sa akin?"

# game/logic/locations/restroom.rpy:126
translate philippine restroom_menu_2_label_363df24c:

    # Don "Haven't I worked hard enough to give you food, clothes and an education?"
    Don "Hindi pa ba ako nagsumikap para mabigyan ka ng pagkain, damit, at edukasyon?"

# game/logic/locations/restroom.rpy:128
translate philippine restroom_menu_2_label_32fefd64:

    # Don "Do you have any idea how much your rehabilitation would have cost?"
    Don "May ideya ka ba kung magkano ang magagastos sa iyong rehabilitasyon?"

# game/logic/locations/restroom.rpy:130
translate philippine restroom_menu_2_label_c152ecb2:

    # ch_Name "No."
    ch_Name "Hindi."

# game/logic/locations/restroom.rpy:132
translate philippine restroom_menu_2_label_bbb98143:

    # ch_Name "All I’m saying is that I didn’t \"choose\" not to go to university."
    ch_Name "Ang sinasabi ko lang ay hindi ko \"pinili\" na hindi pumasok sa unibersidad."

# game/logic/locations/restroom.rpy:134
translate philippine restroom_menu_2_label_3fe05916:

    # ch_Name "It's hard to keep your emotions in check when you're being slung in the face."
    ch_Name "Ang hirap pigilan ang emosyon mo kapag nakasampay ka sa mukha."

# game/logic/locations/restroom.rpy:136
translate philippine restroom_menu_2_label_2d95ecb9:

    # Don "Oh, you poor thing!"
    Don "Oh, kawawa ka!"

# game/logic/locations/restroom.rpy:138
translate philippine restroom_menu_2_label_6c4d7e3c:

    # Don "Is that what you were expecting to hear?"
    Don "Iyan ba ang inaasahan mong marinig?"

# game/logic/locations/restroom.rpy:140
translate philippine restroom_menu_2_label_34919b9e:

    # ch_Name "No, I..."
    ch_Name "Hindi..."

# game/logic/locations/restroom.rpy:142
translate philippine restroom_menu_2_label_0d4f4111:

    # Don "You're 18! Surprise! Nobody owes you anything."
    Don "18 ka na! Walang taong may utang sa iyo."

# game/logic/locations/restroom.rpy:144
translate philippine restroom_menu_2_label_fbfeb7e0:

    # Don "Certainly not me."
    Don "Tiyak na hindi ako."

# game/logic/locations/restroom.rpy:146
translate philippine restroom_menu_2_label_f6e4a5c9:

    # Don "So stop whining and learn to be helpful."
    Don "Kaya itigil mo ang iyong pag-ungol at matuto kang maging matulungin."

# game/logic/locations/restroom.rpy:148
translate philippine restroom_menu_2_label_d39ba59a:

    # ch_Name "I hear you."
    ch_Name "Naririnig kita."

# game/logic/locations/restroom.rpy:150
translate philippine restroom_menu_2_label_02514dd4:

    # Don "So it's a deal, then?"
    Don "May kaugnayan ba tayo?"

# game/logic/locations/restroom.rpy:156
translate philippine restroom_menu_3_label_1b0b05ae:

    # ch_Name "I... I need to think."
    ch_Name "K... kailangan kong mag-isip."

# game/logic/locations/restroom.rpy:158
translate philippine restroom_menu_3_label_4af10fbd:

    # ch_Name "I'm sorry, I'm gonna..."
    ch_Name "Pasensya na, ako ay..."

# game/logic/locations/restroom.rpy:160
translate philippine restroom_menu_3_label_c5b7df6d:

    # Don "Go on, get out of here."
    Don "Sige, umalis ka na dito."

# game/logic/locations/restroom.rpy:162
translate philippine restroom_menu_3_label_1f8ee931:

    # Don "And welcome to adulthood, boy!"
    Don "At maligayang pagdating sa pagtanda!"

# game/logic/locations/restroom.rpy:164
translate philippine restroom_menu_3_label_f708fe02:

    # ch_Name "{i}(Shove your \"welcome to adulthood\" up your ass.){/i}"
    ch_Name "{i}(Ilagay mo nga sa pwet mo ang iyong \"maligayang pagdating sa pagtanda\".){/i}"

# game/logic/locations/restroom.rpy:166
translate philippine restroom_menu_3_label_41ea1485:

    # ch_Name "{i}(Fucking hypocrite.){/i}"
    ch_Name "{i}(Putang ipokrita.){/i}"

# game/logic/locations/restroom.rpy:168
translate philippine restroom_menu_3_label_24687b0f:

    # ch_Name "{i}(I wonder how you'd act if it were Ashley who got hurt instead?){/i}"
    ch_Name "{i}(Ano kaya ang gagawin mo kung si Ashley na lang ang nasaktan?){/i}"

