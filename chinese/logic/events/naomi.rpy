    ch_Name "{i}（这件事已经过去很久了。）{/i}"

# game/logic/events/naomi.rpy:2423
translate chinese Naomi_11_label_76d44268:

    # ch_Name "{i}(His memories should be fine by now.){/i}"
    ch_Name "{i}（他的记忆力现在应该好了。）{/i}"

# game/logic/events/naomi.rpy:2424
